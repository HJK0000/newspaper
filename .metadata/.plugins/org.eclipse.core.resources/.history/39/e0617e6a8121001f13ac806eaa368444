package com.example.api.dto;

import com.example.restAPI.dto.MemberDto;

import lombok.Data;

@Data
public class MemberDto { // DTO(Data Transfer Object) : 계층(레이어드 아키텍쳐)간 데이터 교환(전송)에 사용되는 객체 -- 레이어드 아키텍쳐(계층을 구분지음)
														// 프리젠테이션 - 비즈니스 - 데이터엑세스(dao, jpa쓸때는 repository)
	private String name;
	private String email;
	private String addr;
}
